<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="/public/Home/styles.css">
  </head>
  <body>

    <div class="header" id="header">
      <ul class="logo_items">
        <li id="logo_name"><span class="side_one">Poke</span><span class="side_two">DBase</span></li>
        <li id="logo_img"><img src="/public/pokeball.png"/></li>
      </ul>


      <div class="active_user">
        <ul id="user_accnts" class="user_accnts">
          <li id="username">
            <span id="username_span">
              {{#username}}{{.}}{{/username}}
            </span>
          </li>
          <li>
            {{#userId}}
              <a href="/home/{{.}}">Account</a>
            {{/userId}}
          </li>
        </ul>
      </div>
    </div>


    <div class="home_major">
      <div class="side_block">
        <div class="attr_container">
            <div id="arrow_up"><img src="/upload_attributes/left.png"/></div>
              <div class="attr_listings">
                  <ul id="attr_listings">
                      {{#allElements}}
                      <li><a href="/home/element/{{element}}">{{element}}</a></li>
                      {{/allElements}}
                  </ul>
              </div>
            <div id="arrow_down"><img src="/upload_attributes/right.png"/></div>
        </div>
      </div>

      <div class="pokemon_block">
        <div class="pokemon_listings">
          {{#pokemons}}
          <ul class="pokemon_item"">
            <a href="/home/pokemon/{{id}}">
            <li><img class="home_pokemon_image" src="/{{image}}"></li>
            <li>{{name}}</li>
            </a>
          </ul>
          {{/pokemons}}
        </div>
      </div>
    </div>


  </body>
  <script src="/public/script/jquery-3.2.1.min.js"></script>
  <script src="/public/script/script.js"></script>
</html>
